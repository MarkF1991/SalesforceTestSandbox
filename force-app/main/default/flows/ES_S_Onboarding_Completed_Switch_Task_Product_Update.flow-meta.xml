    <decisions>
        <description>Deciding if there are products already existing and &quot;Active&quot; for the school. This way we can update the previously &quot;Active&quot; products to &quot;Inactive&quot;</description>
        <name>Does_Org_have_existing_active_prod</name>
        <label>Does Org have existing active prod</label>
        <locationX>528</locationX>
        <locationY>791</locationY>
        <defaultConnector>
            <targetReference>Assign_Task_Field_Values</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Is_Status_Active</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Product_Loop.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Active</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_prod_active_ES_S</targetReference>
            </connector>
            <label>Is Status Active</label>
        </rules>
    </decisions>
    <decisions>
        <description>If so, skip, else, proceed.</description>
        <name>Is_prod_active_ES_S</name>
        <label>Is prod active ES-S</label>
        <locationX>352</locationX>
        <locationY>911</locationY>
        <defaultConnector>
            <targetReference>Mark_Inactive</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Prod is not ES-S</defaultConnectorLabel>
        <rules>
            <name>Prod_is_ES_S</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Product_Loop.Feature_Shortname__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>ES-S</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Product_Loop</targetReference>
            </connector>
            <label>Prod is ES-S</label>
        </rules>
    </decisions>
