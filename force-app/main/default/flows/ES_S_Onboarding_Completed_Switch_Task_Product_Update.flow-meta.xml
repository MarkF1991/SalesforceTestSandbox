    <recordLookups>
        <description>Gets the Product records related to the related Account.</description>
        <name>Get_All_Products</name>
        <label>Get All Products</label>
        <locationX>176</locationX>
        <locationY>551</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Product_Loop</targetReference>
        </connector>
        <filterLogic>(1 AND 2 AND 3) AND (4 OR 5 OR 6 OR 7)</filterLogic>
        <filters>
            <field>Organisation__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>Get_Case.AccountId</elementReference>
            </value>
        </filters>
        <filters>
            <field>N4l_Product_Name__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Managed Network to the Classroom</stringValue>
            </value>
        </filters>
        <filters>
            <field>Status__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Inactive</stringValue>
            </value>
        </filters>
        <filters>
            <field>Feature_Shortname__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>HC</stringValue>
            </value>
        </filters>
        <filters>
            <field>Feature_Shortname__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>SNUP</stringValue>
            </value>
        </filters>
        <filters>
            <field>Feature_Shortname__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>WSNUP</stringValue>
            </value>
        </filters>
        <filters>
            <field>Feature_Shortname__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>ES-S</stringValue>
            </value>
        </filters>
        <getFirstRecordOnly>false</getFirstRecordOnly>
        <object>Installed_N4L_Product_Feature__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
