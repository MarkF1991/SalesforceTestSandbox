    <apiVersion>49.0</apiVersion>
    <decisions>
        <name>Does_Account_share_exist</name>
        <label>Does Account share exist?</label>
        <locationX>399</locationX>
        <locationY>149</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Does_Not_Exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AccountShareID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lookup_Group</targetReference>
            </connector>
            <label>Does Not Exist</label>
        </rules>
        <rules>
            <name>Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AccountShareID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Failure</targetReference>
            </connector>
            <label>Exists</label>
        </rules>
    </decisions>
    <description>Checks if an account share already exists between account and public group and if not creates a share.</description>
    <interviewLabel>Create Account Share {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Create Account Share</label>
